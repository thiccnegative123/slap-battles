game.StarterGui:SetCore("SendNotification", {
    Title = "check console for the abilities";
    Text = "Made by Haxarebest1";
    Duration = 5
})
local on = 1
local lol1 = 1
local lol2 = 1
local lol3 = 1
local lol4 = 1
local screenGui = Instance.new("ScreenGui")
screenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
screenGui.ResetOnSpawn = false

local mainFrame = Instance.new("Frame")
mainFrame.Size = UDim2.new(0, 220, 0, 200)
mainFrame.Position = UDim2.new(0.5, -110, 0.4, -100)
mainFrame.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
mainFrame.BorderSizePixel = 2
mainFrame.Visible = false
mainFrame.Parent = screenGui
mainFrame.Active = true
mainFrame.Draggable = true

local searchBar = Instance.new("TextBox")
searchBar.Size = UDim2.new(0, 200, 0, 30)
searchBar.Position = UDim2.new(0, 10, 0, 10)
searchBar.PlaceholderText = "Type to search..."
searchBar.ClearTextOnFocus = false
searchBar.Parent = mainFrame

local suggestionFrame = Instance.new("Frame")
suggestionFrame.Size = UDim2.new(0, 200, 0, 150)
suggestionFrame.Position = UDim2.new(0, 10, 0, 45)
suggestionFrame.BackgroundTransparency = 1
suggestionFrame.Parent = mainFrame

local toggleButton = Instance.new("TextButton")
toggleButton.Size = UDim2.new(0, 100, 0, 30)
toggleButton.Position = UDim2.new(0.5, -50, 0.85, 0)
toggleButton.Text = "Open Search"
toggleButton.Parent = screenGui
toggleButton.Draggable = true

